.goods {
  width: 100%;
  height: 100%;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-color: #fff;
  /* 列表 */
}
.goods .preview {
  height: 750rpx;
  position: relative;
}
.goods .preview image {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.goods .preview .indicator {
  height: 40rpx;
  padding: 0 24rpx;
  line-height: 40rpx;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  color: #fff;
  font-family: Arial, Helvetica, sans-serif;
  background-color: rgba(0, 0, 0, 0.3);
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
}
.goods .preview .indicator .current {
  font-size: 26rpx;
}
.goods .preview .indicator .split {
  font-size: 24rpx;
  margin: 0 1rpx 0 2rpx;
}
.goods .preview .indicator .total {
  font-size: 24rpx;
}
.goods .meta {
  position: relative;
  border-bottom: 1rpx solid #eaeaea;
}
.goods .meta .price {
  height: auto;
  color: #fff;
  font-size: 34rpx;
  padding: 10rpx 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  background-color: #35c8a9;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.goods .meta .price text {
  margin: 10rpx 10rpx;
}
.goods .meta .brand {
  width: 160rpx;
  height: 80rpx;
  overflow: hidden;
  position: absolute;
  top: 26rpx;
  right: 30rpx;
}
.goods .action {
  padding-left: 20rpx;
}
.goods .action .item {
  height: 90rpx;
  padding-right: 60rpx;
  border-bottom: 1rpx solid #eaeaea;
  font-size: 26rpx;
  color: #333;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.goods .action .item:last-child {
  border-bottom: 0 none;
}
.goods .action .label {
  width: auto;
  color: #898b94;
  margin: 0 16rpx 0 10rpx;
}
.goods .action .text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-line-clamp: 1;
}
.goods .list {
  margin: 0 10px;
  padding: 0 20rpx;
  background-color: #f4f4f4;
  margin-bottom: 20rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.goods .list .item {
  line-height: 90rpx;
  padding-left: 10rpx;
  font-size: 17px;
  color: #333;
  border-top: 1rpx solid #ddd;
  position: relative;
  text-align: left;
  -webkit-border-radius: 0;
          border-radius: 0;
  background-color: #f4f4f4;
}
.goods .list .item::after {
  width: auto;
  height: auto;
  left: auto;
  border: none;
}
.goods .list .item:first-child {
  border: none;
}
.goods .list .item::after {
  content: '';
  right: 5rpx;
}
.goods .list .item .num {
  margin-left: 20rpx;
  font-size: 10px;
}
.goods .list .item .price {
  position: absolute;
  right: 70px;
  color: red;
}
.goods .list .item .price::before {
  content: '￥';
  font-size: 80%;
}
.goods .list .item button {
  width: 60px;
  font-size: 10px;
  position: absolute;
  right: 0;
  top: 25%;
  background-color: #ff7d25;
  color: white;
}
.goods .list .common {
  font-size: 13px;
}
.goods .list .common .context {
  width: 195px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goods .list .common .context .time {
  position: absolute;
  right: 0;
}
.goods .u-mode-center-box {
  background-color: #28bb9c;
}
.goods .lis {
  margin: 0 10px;
  padding: 4rpx 40rpx;
  background-color: #efefef;
  margin-top: 30rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.goods .lis .name {
  font-size: 18px;
  margin: 40rpx 0;
}
.goods .lis .useDate {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 15px;
}
.goods .lis .useDate text {
  width: auto;
  margin: auto 0;
}
.goods .lis .useDate .date {
  position: absolute;
  width: auto;
  color: white;
  background-color: #28bb9c;
  right: 80rpx;
  top: -8rpx;
}
.goods .lis .num {
  font-size: 15px;
  margin: 40rpx 0;
}
.goods .lis .num text {
  margin: auto 0;
}
.goods .lis .num u-number-box {
  margin-left: 200rpx;
}
.goods .visitorList {
  position: relative;
  margin: 0 10px;
  padding: 20rpx 40rpx 5rpx 40rpx;
  background-color: #efefef;
  margin-top: 20rpx;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.goods .visitorList .visitors {
  position: relative;
  margin: 20rpx 0;
}
.goods .visitorList .visitors .more {
  position: absolute;
  right: 30rpx;
}
.goods .visitorList .visitors .info {
  position: relative;
  margin: 20rpx 0;
  width: 600rpx;
  height: 100rpx;
  border-top: 1px solid #bfbfbf;
}
.goods .visitorList .visitors .info .clean {
  position: absolute;
  left: 0;
  top: 30%;
}
.goods .visitorList .visitors .info .name {
  position: absolute;
  top: 10rpx;
  left: 100rpx;
}
.goods .visitorList .visitors .info .phone {
  position: absolute;
  bottom: 0;
  left: 100rpx;
}
.goods .visitorList .visitors .info .edit {
  position: absolute;
  right: 0;
  top: 30%;
}
.goods .visitorList .visitors .info .edit_popup .edit_name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 20rpx 20rpx;
}
.goods .visitorList .visitors .info .edit_popup .edit_phone {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 0 20rpx;
}
.goods .visitorList .visitors .info .edit_popup .add-btn {
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  margin: 30rpx 20rpx;
  color: #fff;
  -webkit-border-radius: 80rpx;
          border-radius: 80rpx;
  font-size: 30rpx;
  background-color: #27ba9b;
}
.goods .visitorList .visitors .info .edit_popup .tip {
  width: 90%;
  height: 10%;
  margin: 20rpx auto;
  color: #afafaf;
}
.goods .order {
  background-color: #28bb9c;
  color: white;
  margin: 20rpx 24rpx 0 24rpx;
}

